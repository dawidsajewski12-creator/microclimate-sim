# ============================================
# KOM√ìRKA 15: Monitoring (uruchom i zostaw)
# ============================================
import time
import IPython.display as display

print("üìä Monitoring systemu (od≈õwie≈ºanie co 30s)")
print("=" * 50)

while True:
    try:
        # Pobierz status
        response = requests.get(f"{public_url}/api/status", timeout=5)
        status = response.json()
        
        # Wyczy≈õƒá i wy≈õwietl
        display.clear_output(wait=True)
        
        print(f"üìä Monitoring systemu - {datetime.now().strftime('%H:%M:%S')}")
        print("=" * 50)
        print(f"Status: {status['status']}")
        print(f"Cache: {status['cache_size']} symulacji")
        print(f"Solver: {'‚úÖ Gotowy' if status['solver_ready'] else '‚è≥ Inicjalizacja'}")
        print(f"Dane: {'‚úÖ Za≈Çadowane' if status['data_loaded'] else '‚ùå Brak'}")
        print("=" * 50)
        print(f"API URL: {public_url}")
        print("\nNaci≈õnij STOP aby zatrzymaƒá monitoring")
        
        # Czekaj 30 sekund
        time.sleep(30)
        
    except KeyboardInterrupt:
        print("\n‚èπÔ∏è Monitoring zatrzymany")
        break
    except Exception as e:
        print(f"‚ö†Ô∏è B≈ÇƒÖd: {e}")
        time.sleep(30)
